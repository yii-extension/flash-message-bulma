{
    "name": "yii-extension/flash-message-bulma",
    "type": "library",
    "description": "Flash Message Bulma Widget",
    "keywords": [
        "yii3",
        "widget",
        "flash-message",
        "bulma"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|8.0",
        "yiisoft/session": "^3.0@dev",
        "yiisoft/yii-bulma": "^1.0@dev"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3.0",
        "oomphinc/composer-installers-extender": "^2.0.0",
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.6",
        "vimeo/psalm": "^4.3",
        "yiisoft/di": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Widget\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Widget\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "installer-types": [
            "npm-asset"
        ],
        "installer-paths": {
            "./node_modules/{$name}": [
                "type:npm-asset"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
